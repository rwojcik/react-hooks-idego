{
  "include": [
    "./src/*", "src/hooks/customHooks.useParam", "src/hooks/HelloClassFunction", "src/hooks/useCallback", "src/hooks/memo", "src/hooks/useState", "src/hooks/useEffect", "src/hooks/useLayoutEffect", "src/hooks/useEffectReturn"
  ],
  "compilerOptions": {
    "strict": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
  }
}
